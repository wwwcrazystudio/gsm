{
  "author": "Mikhail Shuvalov",
  "version": "2.2.0",
  "description": "Starter project template boilerplate based on Webpack with SASS/PostCSS and babel ES6/7 support.",
  "browserslist": [
    "last 2 versions",
    "> 5%"
  ],
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@popperjs/core": "^2.10.1",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "aos": "^2.3.4",
    "bootstrap": "^5.0.2",
    "magnific-popup": "^1.1.0",
    "no-ui-slider": "^1.2.0",
    "popper": "^1.0.1",
    "postcss-preset-env": "^6.7.0",
    "rucksack-css": "^1.0.2",
    "swiper": "^6.8.3",
    "webpack-font-preload-plugin": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "ajv": "^8.6.2",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "babel-preset-typescript-vue": "^1.1.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "core-js": "^3.15.2",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "ejs": "^3.1.6",
    "ejs-loader": "^0.5.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^5.3.2",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "imagemin-webp-webpack-plugin": "^3.3.6",
    "mini-css-extract-plugin": "^2.1.0",
    "mocha": "^9.0.2",
    "mocha-loader": "^5.1.5",
    "postcss": "^8.3.6",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-loader": "^6.1.1",
    "sass": "^1.35.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^12.1.0",
    "sass-rem": "^3.0.0",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-webpack-plugin": "^3.0.1",
    "svgo-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.1.4",
    "typescript": "^4.3.5",
    "vue-loader": "^15.9.6",
    "webpack": "^5.45.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0-rc.0",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "template",
    "setup"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:dev": "webpack --config configuration/webpack.dev.config.js --mode=development",
    "watch": "webpack --config configuration/webpack.dev.config.js --mode=development --watch",
    "bundle": "npm install && npm run watch",
    "serve": "webpack serve --config configuration/webpack.dev.config.js --mode=development",
    "build": "webpack --config configuration/webpack.prod.config.js --mode=production",
    "build:wp": "webpack --config configuration/webpack.wp.config.js --mode=production",
    "build:dev_wp": "webpack --config configuration/webpack.wp.config.js --mode=development",
    "lint:sass": "sass-lint -v -q",
    "lint:js": "eslint --ext .js src/assets/js/",
    "stats": "webpack --config configuration/webpack.prod.config.js --mode=production --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json"
  },
  "target": "web"
}
